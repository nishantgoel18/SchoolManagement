 

<h1>Batches</h1>

<div id="batches">
  <% @batches.each do |batch| %>
    <%= render batch %>
    <p>
      <%= link_to "Show this batch", [@school, batch] %>
    </p>
  <% end %>
</div>

<%= link_to 'Home', home_url %> <%= link_to "New batch", new_school_batch_path(@school) if policy(:batch).index? %>
